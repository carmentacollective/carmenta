{
  "name": "carmenta",
  "version": "0.1.0",
  "description": "A heart-centered AI for creating at the speed of thought",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/carmentacollective/carmenta"
  },
  "homepage": "https://carmenta.ai",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@clerk/shared",
      "@sentry/cli",
      "@swc/core",
      "@temporalio/core-bridge",
      "core-js",
      "esbuild",
      "msw",
      "protobufjs",
      "sharp",
      "unrs-resolver"
    ]
  },
  "scripts": {
    "analyze": "next experimental-analyze",
    "analyze:webpack": "ANALYZE=true next build --webpack",
    "build": "next build",
    "render-build": "bash ./scripts/render-deploy.sh",
    "dev": "next dev --turbopack",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev --turbopack",
    "dev:inngest": "npx inngest-cli@latest dev",
    "dev:temporal": "docker compose -f docker/docker-compose.dev.yml up",
    "build:worker": "tsx scripts/build-worker.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "docs:sync": "tsx scripts/sync-docs.ts",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json,css}\"",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "pre-push": "run-p type-check format:check test",
    "prepare": "husky",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "type-check": "tsc --noEmit",
    "eval:nightly": "braintrust eval evals/nightly/eval.ts",
    "eval:routing": "braintrust eval evals/routing/eval.ts",
    "eval:competitive": "braintrust eval evals/competitive/eval.ts",
    "eval:check-regression": "tsx scripts/eval-check-regression.ts",
    "eval:compare-models": "tsx scripts/compare-models.ts",
    "eval:analyze-gaps": "tsx scripts/analyze-gaps.ts"
  },
  "dependencies": {
    "@47ng/simple-e2ee": "^1.1.0",
