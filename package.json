    "marked": "^17.0.1",
    "mathjs": "^15.1.0",
    "nanoid": "^5.1.6",
    "next": "^16.1.1",
    "next-themes": "^0.4.6",
    "next-view-transitions": "^0.3.5",
    "pino": "^10.1.0",
    "postgres": "^3.4.7",
