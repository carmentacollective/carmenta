    "vitest": "^4.0.16"
  },
  "engines": {
    "node": "24.x",
    "pnpm": ">=10.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
